					.row.placeholders
						.col-xs-6.col-sm-3.placeholder
							img.img-responsive(src='/image/dashboard_green.jpg', alt='Generic placeholder thumbnail')
							h4 Label
						.col-xs-6.col-sm-3.placeholder
							img.img-responsive(src='/image/dashboard_green.jpg', alt='Generic placeholder thumbnail')
							h4 Label
							span.text-muted Something else
						.col-xs-6.col-sm-3.placeholder
							img.img-responsive(src='/image/dashboard_blue.jpg', alt='Generic placeholder thumbnail')
							h4 Label
							span.text-muted Something else
						.col-xs-6.col-sm-3.placeholder
							img.img-responsive(src='/image/dashboard_green.jpg', alt='Generic placeholder thumbnail')
							h4 Label
							span.text-muted Something else
					h2.sub-header Total
					.table-responsive
						table.table.table-striped
							thead
								tr
									th #
									th Url
									th Pattern
									th spot
									//- th time
							|
							tbody.thead-light
								- if(url)
									-for (var i =0; i < url.length; i++)
										tr.fuck
											td.danger= i
											td= url[i].url
											td= url[i].pt
											td= url[i].spot
								- else
									tr.fuck
										td.success='N'
										td.success='O'
										td.success='P'
										td.success='E'
							|

